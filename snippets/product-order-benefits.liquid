{% if settings.money_back_title or settings.money_back_content %}
  <div class="money-back-gurantee" style="background-color: {{ settings.money_back_bg }}">
  {% if settings.money_back_image %}
    <div class="money-back-icon"><img src="{{ settings.money_back_image | img_url: '66x66' }}" alt="money back guarantee" width="{{ settings.money_back_image.width }}" height="{{ settings.money_back_image.height }}"> </div>
    {% endif %}
    <div class="money-back-content">
      {% if settings.money_back_title %}
        <div class="mb-title">{{ settings.money_back_title }}</div>
      {% endif %}
      {% if settings.money_back_content %}
        <div class="mb-content">{{ settings.money_back_content }}</div>
      {% endif %}
    </div>
  </div>
  {% endif %}